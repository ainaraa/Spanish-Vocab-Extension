{
  "name": "Spanish Vocab Extension",
  "version": "1.0",
  "description": "See new spanish vocabulary while opening new tabs",
  "permissions": ["tabs"],
  "chrome_url_overrides" : {
    "newtab": "newTab.html"
  },
  "web_accessible_resources": [
    "data/vocab.json",
    "style.css",
    "images/speaker.svg"
  ],
  "manifest_version": 2
}